{
	"name": "sundar-clinic-backend",
	"version": "0.8.3",
	"description": "Server Side Application for Sundar Clinic Website.",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "concurrently \"nodemon server.js\" \"npm run tailwind-w\"",
		"build": "npm run tailwind",
		"tailwind-w": "npx tailwindcss -i ./src/temp/tailwind.css -o ./public/css/common.css --watch",
		"tailwind": "npx tailwindcss -i ./src/temp/tailwind.css -o ./public/css/common.css"
	},
	"keywords": [
		"Express",
		"MERN",
		"Sundar Clinic",
		"Codelance Devs"
	],
	"author": "Kunal Keshan <kunalkeshan12@gmail.com>, Codelance Devs <contact@codelancedevs.com>",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^14.2.0",
		"ejs": "^3.1.6",
		"express": "^4.17.2",
		"html-to-text": "^8.1.0",
		"jsonwebtoken": "^8.5.1",
		"juice": "^8.0.0",
		"mongoose": "^6.1.7",
		"morgan": "^1.10.0",
		"nanoid": "^3.2.0",
		"nodemailer": "^6.7.2",
		"pdfkit": "^0.13.0",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"nodemon": "^2.0.15",
		"postcss": "^8.4.6",
		"tailwindcss": "^3.0.22"
	}
}
